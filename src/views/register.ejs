<!-- <h1 class="mt-5 mb-'">Register</h1> -->
<form action="/register" method="post">
  <div class="container signin-singnup">
    <div class="top"></div>
    <div class="bottom"></div>
    <div class="center">
      <h2>Register</h2>
      <input
        type="text"
        class="form-control"
        id="name"
        name="name"
        placeholder="Name"
      />
      <input
        type="email"
        class="form-control"
        id="email"
        name="email"
        placeholder="Email"
        required
      />
      <input
        type="password"
        class="form-control"
        id="password"
        name="password"
        placeholder="Password"
        required
      />
      <button type="submit" class="btn btn-dark">Register</button>
    </div>
  </div>
</form>
